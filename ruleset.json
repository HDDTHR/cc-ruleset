{
    "name": "Conventional commits",
    "target": "branch",
    "source_type": "Repository",
    "enforcement": "active",
    "rules": [
      {
        "type": "commit_message_pattern",
        "parameters": {
          "name": "",
          "negate": false,
          "pattern": "^((build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test)(\\(\\w+\\))?(!)?(: (.*\\s*)*))|(Merge (.*\\s*)*)|(Initial commit$)",
          "operator": "regex"
        }
      }
    ],
    "bypass_actors": []
  }